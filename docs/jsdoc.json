{
	"opts": {
		"encoding": "utf8",
		"destination": "./jsdoc/",
		"recurse": true,
		"private": true
	},
	"source": {
		"include": ["../server/"],
		"includePattern": ".+\\.js$"
	},
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": ["jsdoc", "closure"]
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": true
	}
}
